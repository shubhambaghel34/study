<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="Logging" type="NServiceBus.Config.Logging, NServiceBus.Core" />
    <section name="UnicastBusConfig" type="NServiceBus.Config.UnicastBusConfig, NServiceBus.Core" />
    <section name="MessageForwardingInCaseOfFaultConfig" type="NServiceBus.Config.MessageForwardingInCaseOfFaultConfig, NServiceBus.Core" />
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  <appSettings>
    <add key="ExternalService.PostEverywhere.Url" value="http://app.posteverywhere.com/webservices/PostLoads.php" />
    <add key="ExternalService.InternetTruckStop.Url" value="http://testws.truckstop.com:8080/V13/Posting/LoadPosting.svc" />
    <add key="ServiceName" value="Coyote.Execution.Posting.Endpoint" />
    <add key="NoReplyEmail" value="DoNotReply@coyotelogistics.com" />
    <add key="ServiceControl/Queue" value="Particular.ServiceControl" />
    <add key="NewRelic.AgentEnabled" value="false" />
    <add key="NewRelic.AppName" value="Coyote.Execution.Posting.Endpoint" />
  </appSettings>
  <connectionStrings>
    <add name="NServiceBus/Transport" connectionString="host=qagxguru02;port=5672;username=guest;password=guest;" />
    <add name="NServiceBus/Persistence" connectionString="Data Source=devgxsql07;Initial Catalog=Guru_QA_NServiceBus;User Id=BazookaAdmin;Password=c0y0te;Connect Timeout=15;Encrypt=False;TrustServerCertificate=False" providerName="System.Data.SqlClient" />
    <add name="Integrated.BazookaDbContext" connectionString="Data Source=devgxsql07;Initial Catalog=Guru_QA;Integrated Security=SSPI;MultipleActiveResultSets=True" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <MessageForwardingInCaseOfFaultConfig ErrorQueue="error" />
  <Logging Threshold="DEBUG" />
  <UnicastBusConfig>
    <MessageEndpointMappings>
      <add Messages="Coyote.Execution.Posting.Contracts" Endpoint="Coyote.Execution.Posting.Endpoint" />
    </MessageEndpointMappings>
  </UnicastBusConfig>
  <log4net>
    <root>
      <level value="INFO" />
      <appender-ref ref="LogFileAppender" />
      <appender-ref ref="EventLogAppender" />
    </root>
    <appender name="LogFileAppender" type="log4net.Appender.RollingFileAppender">
      <file value="c:\logs\Coyote.Execution.Posting.Endpoint.log" />
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
      <appendToFile value="true" />
      <rollingStyle value="Composite" />
      <datePattern value="yyyyMMdd" />
      <maxSizeRollBackups value="10" />
      <maximumFileSize value="10MB" />
      <staticLogFileName value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date | %-5level | E%-6property{ErrorId} | W%-6property{UnitOfWorkId} | T%-6thread | P%t | %message%newline%exception" />
      </layout>
    </appender>
    <appender name="EventLogAppender" type="log4net.Appender.EventLogAppender">
      <logName value="Coyote" />
      <applicationName value="Coyote.Execution.Posting.Endpoint.EventLog" />
      <layout type="log4net.Layout.PatternLayout">
        <ConversionPattern value="%date | %-5level | E%-6property{ErrorId} | W%-6property{UnitOfWorkId} | T%-6thread | %stacktrace{10} | %message%newline%exception" />
      </layout>
      <filter type="log4net.Filter.LevelRangeFilter">
        <levelMin value="ERROR" />
        <levelMax value="FATAL" />
      </filter>
    </appender>
  </log4net>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="log4net" publicKeyToken="669e0ddf0bb1aa2a" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-2.0.8.0" newVersion="2.0.8.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-11.0.0.0" newVersion="11.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Autofac" publicKeyToken="17863af14b0044da" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.3.0.0" newVersion="4.3.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Net.Http.Formatting" publicKeyToken="31bf3856ad364e35" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-5.2.4.0" newVersion="5.2.4.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Web.Http" publicKeyToken="31bf3856ad364e35" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-5.2.3.0" newVersion="5.2.3.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="RabbitMQ.Client" publicKeyToken="89e7d7c5feba84ce" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-5.0.0.0" newVersion="5.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>